{
  "manifest_version": 3,

  "name": "Chrome Extension TypeScript Starter",
  "description": "Chrome Extension, TypeScript, Visual Studio Code",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["https://github.com/*"],
          "exclude_matches": ["https://github.com/*/*"],
          "js": ["js/content_script.js"]
      }
  ],

  "host_permissions": [
    "<all_urls>"
  ]
}
